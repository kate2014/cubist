    "css-loader": "0.23.0",
    "eslint": "1.8.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-mocha": "1.0.0",
    "eslint-plugin-react": "3.6.3",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.4",
    "gulp": "3.9.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.0",
    "json-loader": "0.5.3",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
