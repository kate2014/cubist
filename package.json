{
  "name": "cubist",
  "version": "1.0.0",
  "description": "Open Source 3D Model Web Gallery",
  "scripts": {
    "lint": "eslint src/** || true",
    "lint-ci": "eslint src/**",
    "sasslint": "sass-lint \"src/webapp/app/styles/**/*.scss\" -v || true",
    "sasslint-ci": "sass-lint \"src/webapp/app/styles/**/*.scss\"",
    "test": "NODE_PATH=src mocha tests/**/*.spec.js --compilers js:babel-register --recursive --reporter spec --require tests/init.js || true",
    "test-ci": "NODE_PATH=src mocha tests/**/*.spec.js --compilers js:babel-register --recursive --reporter spec --require tests/init.js",
    "cover": "NODE_PATH=src babel-node ./node_modules/isparta/bin/isparta cover --report html _mocha -- tests/**/*.spec.js --recursive --reporter nyan --require tests/init.js || true",
    "cover-ci": "NODE_PATH=src babel-node ./node_modules/isparta/bin/isparta cover _mocha -- tests/**/*.spec.js --recursive --reporter spec --require tests/init.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "webapp-dev": "NODE_PATH=src gulp webapp-dev",
    "webpapp-prod": "NODE_PATH=src gulp webapp-prod",
    "api-dev": "NODE_PATH=src gulp api-dev",
    "api-prod": "NODE_PATH=src gulp api-prod",
    "pm2": "NODE_PATH=src gulp pm2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nus-mtp/cubist.git"
  },
  "keywords": [
    "react",
    "universal",
    "nodejs",
    "redux",
    "3d",
    "threejs",
    "opensource"
  ],
  "author": "Studio/Slash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nus-mtp/cubist/issues"
  },
